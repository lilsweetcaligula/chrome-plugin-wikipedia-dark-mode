{
  "manifest_version": 3,

  "name": "Wikipedia Dark Mode",
  "description": "This extension will enable dark mode for Wikipedia",
  "version": "1.0",

  "content_scripts": [
     {
        "matches":    ["https://en.wikipedia.org/*"],
        "css":        ["dark_wiki.css"],
        "js":         ["dark_wiki_script.js"]
     }
  ]
}
